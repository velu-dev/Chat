<div class="card">
  <div class="card-block">
    <div class="row">
      <div class="col-md-1">
       <%=image_tag message.user.profile_pic.thumb%>
      </div>
      <div class="col-md-11">
        <p class="card-text">
          <strong><span class="text-muted"><%=link_to message.user.name, homes_profile_path %></strong> at <%= message.timestamp %> says</span><br>
          <%= message.body %>
        </p>
      </div>
    </div>
  </div>
</div>
